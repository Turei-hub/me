{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Run FastAPI (uvicorn)",
        "type": "python",
        "request": "launch",
        "module": "uvicorn",
        "args": [
          "backend_api:app",
          "--reload",
          "--host", "127.0.0.1",
          "--port", "8000"
        ],
        "jinja": true,
        "envFile": "${workspaceFolder}/.env"
      }
    ]
  }
